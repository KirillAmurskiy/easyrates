{
  "ConnectionStrings": {
    "DefaultConnection": "server=localhost;port=3306;database=socialnetwork;user=socialnetwork_app;password=socialnetwork_app"
  },

  "ProviderInMemory": {
    "Enabled": "true"
  },

  "ProviderOpenExchange": {
    "Enabled": "true",
    "AppId": "ae0227f947ee4c6f9e474560377455df"
  },

  "Timetable": {
    "UpdatePeriod": "01:00:00",
    "AnchorTime": "00:01:00"
  },
  
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Debug"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/logs-.log",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
